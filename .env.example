# Network Configuration
NODE_ENV=development
SOLANA_NETWORK=devnet

# RPC Endpoints
SOLANA_RPC_ENDPOINT=https://api.devnet.solana.com
SOLANA_RPC_ENDPOINT_BACKUP=https://devnet.solana.rpcpool.com

# Monitoring
SENTRY_DSN=your-sentry-dsn
LOG_LEVEL=debug
WEBHOOK_URL=your_webhook_url_here

# Security
PRIVATE_KEY_PASSWORD=your-private-key-password

# Rate Limits
MAX_TRANSACTIONS_PER_SECOND=10
MAX_CONCURRENT_REQUESTS=5

# Swap Configuration
MINIMUM_LIQUIDITY_THRESHOLD=1000
MAX_PRICE_IMPACT_PERCENT=5
SLIPPAGE_TOLERANCE_PERCENT=1

# Reward Distribution
MINIMUM_REWARD_AMOUNT=0.00001
MINIMUM_TOKEN_HOLDING=50000
DISTRIBUTION_INTERVAL=3600 # in seconds

# Emergency Settings
EMERGENCY_WALLET=your-emergency-wallet-address
PAUSE_THRESHOLD_PRICE_CHANGE=10 # percentage 